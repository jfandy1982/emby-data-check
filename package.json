  },
  "scripts": {
    "build:affected": "nx affected --parallel --targets=build",
    "build:all": "nx run-many --parallel --targets=build --all",
    "checks:all:conf:default": "nx run-many --parallel --targets=typecheck,lint,build,test,e2e",
    "checks:all:conf:dev": "nx run-many --parallel --targets=typecheck,lint,build,test,e2e --configuration=development",
    "checks:all:conf:prod": "nx run-many --parallel --targets=typecheck,lint,build,test,e2e --configuration=production",
    "cleanup:all": "npm run cleanup:generated && npm run cleanup:deps && npm ci",
    "cleanup:cache:angular": "rimraf ./.angular",
    "cleanup:cache:nx": "rimraf ./.nx/cache",
    "cleanup:caches": "npm run cleanup:cache:angular && npm run cleanup:cache:nx",
    "cleanup:deps": "rimraf ./node_modules",
    "cleanup:generated": "npm run cleanup:caches && rimraf ./coverage && rimraf ./cypress && rimraf ./dist",
    "coverage:merge": "npx lcov-result-merger \"coverage/apps/*/lcov.info\" > coverage/merged-lcov.info",
    "e2e:api": "npx nx e2e edc-api-e2e",
    "e2e:api:ci": "start-server-and-test 'npx nx serve edc-api --configuration=production' http://localhost:3000/api 'npx nx e2e edc-api-e2e'",
    "e2e:api:watch": "npx nx e2e edc-api-e2e --watch",
    "e2e:ui:all": "npx nx run-many --targets=e2e --projects=edc-ui-ng-*-e2e",
    "e2e:ui:cucumber": "npx nx e2e edc-ui-ng-cucumber-e2e",
    "e2e:ui:cucumber:ci": "npx nx e2e-ci edc-ui-ng-cucumber-e2e",
    "e2e:ui:cucumber:open": "npx nx open-cypress edc-ui-ng-cucumber-e2e",
    "e2e:ui:cypress": "npx nx e2e edc-ui-ng-cypress-e2e",
    "e2e:ui:cypress:ci": "npx nx e2e-ci edc-ui-ng-cypress-e2e",
    "e2e:ui:cypress:open": "npx nx open-cypress edc-ui-ng-cypress-e2e",
    "format:affected": "nx format:write",
    "format:all": "nx format:write --all",
    "format:all:check": "nx format:check --all",
    "lint:affected:check": "nx affected --parallel --targets=lint",
    "lint:all:check": "nx run-many --parallel --targets=lint --all",
    "lint:npmPkgJsonLint:check": "npmPkgJsonLint --configFile .github/linters/.npmpackagejsonlintrc.json .",
    "ng": "nx",
    "nx": "nx",
    "prepare": "husky || true",
    "spell:check": "cspell lint --dot --show-suggestions --no-progress --no-must-find-files --config ./.cspell.json",
    "spell:check:all": "npm run spell:check .",
    "spell:search:dict": "cspell trace --all --config ./.cspell.json",
    "stack:api": "nx run edc-api:stack",
    "stack:ng": "nx run edc-ui-ng:stack",
    "test:affected": "nx affected --parallel --targets=test",
    "test:all": "nx run-many --parallel --targets=test --all",
    "test:all:ci": "nx run-many --parallel --targets=test --all",
    "typecheck:affected": "nx affected --parallel --targets=typecheck",
    "typecheck:all": "nx run-many --parallel --targets=typecheck --all",
    "upgrade:nx:doit": "rimraf ./package-lock.json && npm run cleanup:deps && npm install && nx migrate --run-migrations --if-exists",
    "upgrade:nx:prepare": "nx migrate latest --interactive"
  },
  "version": "0.0.1-SNAPSHOT"
}
