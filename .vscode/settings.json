{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.organizeImports": "never",
    "source.sortImports": "never",
    "source.addMissingImports": "never",
    "source.removeUnusedImports": "never"
  },
  "eslint.format.enable": false,
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact"],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/.git/**": true,
    "**/.idea/**": true,
    "**/.nx/**": true,
    "**/.nyc_output/**": true,
    "**/.angular/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/tmp/**": true,
    "**/out-tsc/**": true,
    "**/*.tsbuildinfo": true,
    "**/coverage/**": true,
    "**/cypress/videos/**": true,
    "**/cypress/screenshots/**": true,
    "**/cypress/downloads/**": true
  },
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/.idea/**": true,
    "**/node_modules/**": true,
    "**/.nx/**": true,
    "**/dist/**": true,
    "**/tmp/**": true,
    "**/out-tsc/**": true,
    "**/*.tsbuildinfo": true,
    "**/coverage/**": true,
    "**/.angular/**": true,
    "**/.nyc_output/**": true,
    "**/cypress/videos/**": true,
    "**/cypress/screenshots/**": true,
    "**/cypress/downloads/**": true,
    "**/migrations.json": true,
    "**/experiments/**": true
  },
  "search.exclude": {
    "**/.git/**": true,
    "**/.idea/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/tmp/**": true,
    "**/out-tsc/**": true,
    "**/*.tsbuildinfo": true,
    "**/coverage/**": true,
    "**/.nx/**": true,
    "**/.angular/**": true,
    "**/.nyc_output/**": true,
    "**/cypress/videos/**": true,
    "**/cypress/screenshots/**": true,
    "**/cypress/downloads/**": true
  }
}
